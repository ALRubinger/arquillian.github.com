    - identity = site.identities.lookup(username)
    %li
      -#%a{:href=>identity.jbosscommunity.profile_url}<
      %a{:href=>identity.github.profile_url}<
        %img{:src=>identity.avatar_url(60), :width=>60, :height=>60, :alt=>identity.name}
      %h3= identity.name
      %strong= identity.role
      %em= identity.location
  %li
    %a{:href=>'http://github.com/arquillian'}
      %img{:src=>'http://gravatar.com/avatar/9be5bfc3104c3702712413ff986fe45b?s=60', :width=>60, :height=>60, :alt=>'Ike'}
